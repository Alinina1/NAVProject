<+>@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@700&display=swap');\r\n@import url('https://fonts.googleapis.com/css2?family=Bad+Script&display=swap');\r\nbody{\r\n    /* background-color: #D6C7B2; */\r\n    min-height: 100%;\r\n    margin:0;\r\n    padding:0;\r\n}\r\n\r\nhtml{\r\n    margin:0;\r\n    padding:0;\r\n}\r\n\r\n/* ШАПКА +++*/\r\n\r\n.panel{\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 20;\r\n    left: 0px;\r\n    top: 0px;\r\n    background-color: #D6C7B2;\r\n    border-bottom: 2px solid #494440;\r\n}\r\n\r\n.panel a {\r\n    float: left;\r\n    color: #494440;\r\n    text-align: center;\r\n    padding: 14px 16px;\r\n    text-decoration: none;\r\n    font-size: 17px;\r\n    font-family: 'Montserrat', sans-serif;\r\n}\r\n\r\n.panel a:hover {\r\n    background-color: #9e8976;\r\n    color: black;\r\n}\r\n.panel .logo:hover {\r\n    background-color: #D6C7B2;\r\n    color: black;\r\n}\r\n\r\n.panel a.logo {\r\n    width: 200px;\r\n    height: 20px;\r\n    padding-left: 80px;\r\n    background-image: url(\"../img/logo.png\");\r\n    background-size: 100px;\r\n    background-repeat: no-repeat;\r\n    background-position-y: center;\r\n    background-position-x: center;\r\n}\r\n\r\n/*Всплывающие окошки + */\r\n.dropdown {\r\n    float: left;\r\n    overflow: hidden;\r\n}\r\n.dropdown .dropbtn {\r\n    font-size: 17px;    \r\n    border: none;\r\n    outline: none;\r\n    color: #494440;\r\n    padding: 14px 16px;\r\n    background-color: inherit;\r\n    font-family: 'Montserrat', sans-serif;\r\n    margin: 0;\r\n}\r\n.panel a:hover, .dropdown:hover .dropbtn {\r\n    background-color: #9e8976;\r\n}\r\n.dropdown-content {\r\n    display: none;\r\n    position: absolute;\r\n    background-color: #9e8976;\r\n    min-width: 160px;\r\n    z-index: 1;\r\n}\r\n.dropdown-content a {\r\n    float: none;\r\n    color: white;\r\n    padding: 12px 16px;\r\n    text-decoration: none;\r\n    display: block;\r\n    text-align: left;\r\n    font-size: 13px;\r\n}\r\n.dropdown-content a:hover {\r\n    background-color: #9e8976;\r\n}\r\n.dropdown:hover .dropdown-content {\r\n    display: block;\r\n}\r\n/*Всплывающие окошки - */\r\n\r\n/*Кнопка для входа в аккаунт +*/\r\n.btn {\r\n    box-sizing: border-box;\r\n    background-color: transparent;\r\n    border: 2px solid #e74c3c;\r\n    border-radius: 0.6em;\r\n    cursor: pointer;\r\n    \r\n    display: flex;\r\n    -webkit-align-self: center;\r\n        -ms-flex-item-align: center;\r\n            align-self: center;\r\n    font-size: 13px;\r\n\r\n    padding: 0.5em 1.5em;\r\n    text-decoration: none;\r\n    text-align: center;\r\n    text-transform: uppercase;\r\n    font-family: 'Montserrat', sans-serif;\r\n}\r\n\r\n.btn:hover, .btn:focus {\r\n    color: #fff;\r\n    outline: 0;\r\n  }\r\n\r\n.fourth {\r\n    margin-top: 6px;\r\n    border-color: #75859e;\r\n    color: #fff;\r\n    background-image: -webkit-linear-gradient(45deg, #75859e 50%, transparent 50%);\r\n    background-image: linear-gradient(45deg, #75859e 50%, transparent 50%);\r\n    background-position: 100%;\r\n    background-size: 400%;\r\n  }\r\n  .fourth:hover {\r\n    background-position: 0;\r\n  }\r\n\r\n  #guest-kniga{\r\n      margin-right: 140px;\r\n  }\r\n/*Кнопка для входа в аккаунт -*/\r\n\r\n/* ШАПКА ---*/\r\n\r\n\r\n/* СТРАНИЦА О СЕБЕ +++ */\r\n\r\n/*страница 1*/\r\n.page1{\r\n    /*background-image: url(\"../img/school-fon1.jpg\");\r\n    background-size: 100%;\r\n    height: 709px;\r\n    background-repeat: repeat-x;\r\n    */\r\n    background-color: #D6C7B2;\r\n    background-size: auto;\r\n    height: 300px;\r\n    position: relative;\r\n}\r\n.fone1 {    \r\n    padding: 2em 1.5em;\r\n    text-decoration: none;\r\n    text-align: left;\r\n    text-transform: uppercase;\r\n    font-family: 'Montserrat', sans-serif;\r\n    font-size: 60px;\r\n    color: #7c695b;\r\n    position: absolute;\r\n}\r\n#fio-anim:hover{\r\n    font-size: 70px;\r\n}\r\n#info_about_me{\r\n    